<?xml version="1.0" ?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
	<edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Core.V1.xml">
		<edmx:Include Namespace="Org.OData.Core.V1" Alias="OData"/>
	</edmx:Reference>
	<edmx:Reference Uri="http://docs.oasis-open.org/odata/odata/v4.0/errata03/csd01/complete/vocabularies/Org.OData.Capabilities.V1.xml">
		<edmx:Include Namespace="Org.OData.Capabilities.V1" Alias="Capabilities"/>
	</edmx:Reference>
	<edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishExtensions_v1.xml">
		<edmx:Include Namespace="RedfishExtensions.v1_0_0" Alias="Redfish"/>
	</edmx:Reference>
	<edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/RedfishYangExtensions_v1.xml">
		<edmx:Include Namespace="RedfishYangExtensions.v1_0_0" Alias="RedfishYang"/>
	</edmx:Reference>
	<edmx:Reference Uri="http://redfish.dmtf.org/schemas/v1/Resource_v1.xml">
		<edmx:Include Namespace="Resource.v1_0_0"/>
	</edmx:Reference>
	<edmx:DataServices>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_interfaces.interfaces_state.interface.ipv6.address">
			<Annotation Term="Redfish.OwningEntity" String="DMTF"/>
			<EntityType Name="address" Abstract="true" BaseType="Resource.v1_0_0.Resource">
				<Annotation Term="OData.Description" String="Parameters for address."/>
				<Annotation Term="OData.LongDescription" String="Parameters for address."/>
				<Annotation Term="Capabilities.InsertRestrictions">
					<Record>
						<PropertyValue Property="Insertable" Bool="false"/>
					</Record>
				</Annotation>
				<Annotation Term="Capabilities.UpdateRestrictions">
					<Record>
						<PropertyValue Property="Updatable" Bool="false"/>
					</Record>
				</Annotation>
				<Annotation Term="Capabilities.DeleteRestrictions">
					<Record>
						<PropertyValue Property="Deletable" Bool="false"/>
					</Record>
				</Annotation>
			</EntityType>
		</Schema>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_interfaces.interfaces_state.interface.ipv6.address.v1_0_0">
			<Annotation Term="Redfish.OwningEntity" String="DMTF"/>
			<Annotation Term="RedfishYang.key" String="ip"/>
			<Annotation Term="OData.Description" String="The list of IPv6 addresses on the interface."/>
			<EnumType Name="status">
				<Member Name="preferred">
					<Annotation Term="OData.Description" String="This is a valid address that can appear as the destination or source address of a packet."/>
				</Member>
				<Member Name="deprecated">
					<Annotation Term="OData.Description" String="This is a valid but deprecated address that should no longer be used as a source address in new communications, but packets addressed to such an address are processed as expected."/>
				</Member>
				<Member Name="invalid">
					<Annotation Term="OData.Description" String="This isn't a valid address, and it shouldn't appear as the destination or source address of a packet."/>
				</Member>
				<Member Name="inaccessible">
					<Annotation Term="OData.Description" String="The address is not accessible because the interface to which this address is assigned is not operational."/>
				</Member>
				<Member Name="unknown">
					<Annotation Term="OData.Description" String="The status cannot be determined for some reason."/>
				</Member>
				<Member Name="tentative">
					<Annotation Term="OData.Description" String="The uniqueness of the address on the link is being verified.  Addresses in this state should not be used for general communication and should only be used to determine the uniqueness of the address."/>
				</Member>
				<Member Name="duplicate">
					<Annotation Term="OData.Description" String="The address has been determined to be non-unique on the link and so must not be used."/>
				</Member>
				<Member Name="optimistic">
					<Annotation Term="OData.Description" String="The address is available for use, subject to restrictions, while its uniqueness on a link is being verified."/>
				</Member>
				<Collection>
					<Record>
						<Annotation Term="RedfishYang.enum" String="preferred"/>
					</Record>
					<Record>
						<Annotation Term="RedfishYang.enum" String="deprecated"/>
					</Record>
					<Record>
						<Annotation Term="RedfishYang.enum" String="invalid"/>
					</Record>
					<Record>
						<Annotation Term="RedfishYang.enum" String="inaccessible"/>
					</Record>
					<Record>
						<Annotation Term="RedfishYang.enum" String="unknown"/>
					</Record>
					<Record>
						<Annotation Term="RedfishYang.enum" String="tentative"/>
					</Record>
					<Record>
						<Annotation Term="RedfishYang.enum" String="duplicate"/>
					</Record>
					<Record>
						<Annotation Term="RedfishYang.enum" String="optimistic"/>
					</Record>
				</Collection>
				<Annotation Term="OData.Description" String="The status of an address.  Most of the states correspond to states from the IPv6 Stateless Address Autoconfiguration protocol."/>
				<Annotation Term="RedfishYang.reference" String="RFC 4293: Management Information Base for the           Internet Protocol (IP)           - IpAddressStatusTC RFC 4862: IPv6 Stateless Address Autoconfiguration"/>
			</EnumType>
			<EntityType Name="address" BaseType="ietf_interfaces.interfaces_state.interface.ipv6.address.address">
				<Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/list"/>
				<Property Name="ip" Type="inet.ipv6_address_no_zone">
					<Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="inet.YangType" EnumMember="inet.YangTypes/ipv6_address_no_zone"/>
					<Annotation Term="OData.Description" String="The IPv6 address on the interface."/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
				<Property Name="prefix_length" Type="Edm.Byte">
					<Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/uint8">
						<Annotation Term="RedfishYang.range" String="0..128"/>
					</Annotation>
					<Annotation Term="RedfishYang.mandatory" EnumMember="RedfishYang.Mandatory/true"/>
					<Annotation Term="OData.Description" String="The length of the subnet prefix."/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
				<Property Name="origin" Type="RedfishYang.ip_address_origin">
					<Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/ip_address_origin"/>
					<Annotation Term="OData.Description" String="The origin of this address."/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
				<Property Name="status" Type="ietf_interfaces.interfaces_state.interface.ipv6.address.v1_0_0.status">
					<Annotation Term="RedfishYang.NodeType" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="RedfishYang.YangType" EnumMember="RedfishYang.YangTypes/enumeration"/>
					<Annotation Term="OData.Description" String="The status of an address.  Most of the states correspond to states from the IPv6 Stateless Address Autoconfiguration protocol."/>
					<Annotation Term="RedfishYang.reference" String="RFC 4293: Management Information Base for the           Internet Protocol (IP)           - IpAddressStatusTC RFC 4862: IPv6 Stateless Address Autoconfiguration"/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
			</EntityType>
		</Schema>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_interfaces.interfaces_state.interface.ipv6.addressCollection">
			<Annotation Term="Redfish.OwningEntity" String="DMTF"/>
			<EntityType Name="addressCollection" BaseType="Resource.v1_0_0.ResourceCollection">
				<Annotation Term="OData.Description" String="A Collection of address resource instances."/>
				<Annotation Term="Capabilities.InsertRestrictions">
					<Record>
						<PropertyValue Property="Insertable" Bool="false"/>
					</Record>
				</Annotation>
				<Annotation Term="Capabilities.UpdateRestrictions">
					<Record>
						<PropertyValue Property="Updatable" Bool="false"/>
					</Record>
				</Annotation>
				<Annotation Term="Capabilities.DeleteRestrictions">
					<Record>
						<PropertyValue Property="Deletable" Bool="false"/>
					</Record>
				</Annotation>
				<NavigationProperty Name="Members" Type="Collection(address.address)">
					<Annotation Term="OData.Permissions" EnumMember="OData.Permissions/Read"/>
					<Annotation Term="OData.Description" String="Contains members of this collection."/>
					<Annotation Term="OData.AutoExpandReferences"/>
				</NavigationProperty>
			</EntityType>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>

