	</edmx:Reference>
	<edmx:DataServices>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_interfaces.interfaces.interface.ipv6.autoconf">
			<Annotation Term="Redfish.OwningEntity" String="DMTF"/>
			<EntityType Name="autoconf" Abstract="true" BaseType="Resource.v1_0_0.Resource">
				<Annotation Term="OData.Description" String="Parameters for autoconf."/>
				<Annotation Term="OData.LongDescription" String="Parameters for autoconf."/>
				<Annotation Term="Capabilities.InsertRestrictions">
					<Record>
						<PropertyValue Property="Insertable" Bool="false"/>
					</Record>
				</Annotation>
				<Annotation Term="Capabilities.UpdateRestrictions">
					<Record>
						<PropertyValue Property="Updatable" Bool="false"/>
					</Record>
				</Annotation>
				<Annotation Term="Capabilities.DeleteRestrictions">
					<Record>
						<PropertyValue Property="Deletable" Bool="false"/>
					</Record>
				</Annotation>
			</EntityType>
		</Schema>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ietf_interfaces.interfaces.interface.ipv6.autoconf.v1_0_0">
			<Annotation Term="Redfish.OwningEntity" String="DMTF"/>
			<Annotation Term="OData.Description" String="Parameters to control the autoconfiguration of IPv6 addresses, as described in RFC 4862."/>
			<Annotation Term="RedfishYang.reference" String="RFC 4862: IPv6 Stateless Address Autoconfiguration"/>
			<EntityType Name="autoconf" BaseType="ietf_interfaces.interfaces.interface.ipv6.autoconf.autoconf">
				<Annotation Term="RedfishYang.NodeTypes" EnumMember="RedfishYang.NodeTypes/container"/>
				<Property Name="create_global_addresses" Type="Edm.Boolean" DefaultValue="true">
					<Annotation Term="RedfishYang.NodeTypes" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="RedfishYang.YangType" String="boolean"/>
					<Annotation Term="RedfishYang.default" String="true"/>
					<Annotation Term="OData.Description" String="If enabled, the host creates global addresses as described in RFC 4862."/>
					<Annotation Term="RedfishYang.reference" String="RFC 4862: IPv6 Stateless Address Autoconfiguration           Section 5.5"/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
				<Property Name="create_temporary_addresses" Type="Edm.Boolean" DefaultValue="false">
					<Annotation Term="RedfishYang.NodeTypes" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="RedfishYang.if_feature" String="ipv6-privacy-autoconf"/>
					<Annotation Term="RedfishYang.YangType" String="boolean"/>
					<Annotation Term="RedfishYang.default" String="false"/>
					<Annotation Term="OData.Description" String="If enabled, the host creates temporary addresses as described in RFC 4941."/>
					<Annotation Term="RedfishYang.reference" String="RFC 4941: Privacy Extensions for Stateless Address           Autoconfiguration in IPv6"/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
				<Property Name="temporary_valid_lifetime" Type="RedfishYang.uint32" DefaultValue="604800">
					<Annotation Term="RedfishYang.NodeTypes" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="RedfishYang.if_feature" String="ipv6-privacy-autoconf"/>
					<Annotation Term="RedfishYang.YangType" String="uint32"/>
					<Annotation Term="RedfishYang.units" String="seconds"/>
					<Annotation Term="RedfishYang.default" String="604800"/>
					<Annotation Term="OData.Description" String="The time period during which the temporary address is valid."/>
					<Annotation Term="RedfishYang.reference" String="RFC 4941: Privacy Extensions for Stateless Address           Autoconfiguration in IPv6           - TEMP_VALID_LIFETIME"/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
				<Property Name="temporary_preferred_lifetime" Type="RedfishYang.uint32" DefaultValue="86400">
					<Annotation Term="RedfishYang.NodeTypes" EnumMember="RedfishYang.NodeTypes/leaf"/>
					<Annotation Term="RedfishYang.if_feature" String="ipv6-privacy-autoconf"/>
					<Annotation Term="RedfishYang.YangType" String="uint32"/>
					<Annotation Term="RedfishYang.units" String="seconds"/>
					<Annotation Term="RedfishYang.default" String="86400"/>
					<Annotation Term="OData.Description" String="The time period during which the temporary address is preferred."/>
					<Annotation Term="RedfishYang.reference" String="RFC 4941: Privacy Extensions for Stateless Address           Autoconfiguration in IPv6           - TEMP_PREFERRED_LIFETIME"/>
					<Annotation Term="OData.Permissions" EnumMember="OData.Permission/Read"/>
				</Property>
